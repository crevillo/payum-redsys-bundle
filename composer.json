{
    "name": "crevillo/payum-redsys-bundle",
    "type": "symfony-bundle",
    "description": "Redsys integration with payum",
    "keywords": [
        "symfony",
        "payment",
        "redsys"
    ],
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Revillo",
            "email": "crevillo@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "http://payum.org/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/PayumBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "payum/payum-bundle": "0.13.*@dev",
        "crevillo/payum-redsys": "0.13.*@dev"
    },
    "autoload": {
        "psr-0": { "Crevillo\\Bundle\\PayumRedsysBundle": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.13-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Crevillo/Bundle/PayumRedsysBundle"
}
